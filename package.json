{
  "name": "website-fe",
  "version": "2.0.0",
  "author": {
    "name": "David Bradshaw",
    "url": "https://davidbradshaw.us"
  },
  "scripts": {
    "ng": "ng",
    "dev": "npm run set:hosts && ng serve -o --host davidtest.io",
    "build": "rm -rf dist && ng build --configuration production",
    "lint": "ng lint",
    "set:hosts": "if ! hostile list | grep \"127.0.0.1 davidtest.io\" >/dev/null; then sudo hostile set 127.0.0.1 davidtest.io; fi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.7",
    "font-awesome": "^4.7.0",
    "jquery": "^3.7.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.1.1",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@angular/language-service": "^20.1.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "hostile": "^1.4.0",
    "typescript": "^5.8.3"
  }
}
