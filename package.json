{
  "name": "website-fe",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "npm run set:hosts && ng serve -o --host davidtest.io",
    "build": "rm -rf dist && ng build --configuration production",
    "lint": "ng lint",
    "set:hosts": "if ! hostile list | grep \"127.0.0.1 davidtest.io\" >/dev/null; then sudo hostile set 127.0.0.1 davidtest.io; fi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.4",
    "@angular/common": "^15.2.4",
    "@angular/compiler": "^15.2.4",
    "@angular/core": "^15.2.4",
    "@angular/forms": "^15.2.4",
    "@angular/platform-browser": "^15.2.4",
    "@angular/platform-browser-dynamic": "^15.2.4",
    "@angular/router": "^15.2.4",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.4",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.4",
    "@angular/language-service": "^15.2.4",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "hostile": "^1.3.3",
    "typescript": "^4.9.5"
  }
}
